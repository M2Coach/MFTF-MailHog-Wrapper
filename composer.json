{
    "name": "m2coach/mftf-mailhog-wrapper",
    "description": "Verify your e-mails after being delivered with Mailhog wrapper.",
    "require": {
        "php": "^7.1",
        "magento/magento2-functional-testing-framework": "^2.3.0",
        "magento/magento-composer-installer": "*"
    },
    "type": "magento2-module",
    "version": "0.1.1",
    "license": [
        "MIT"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "M2Coach\\MFTFMailHogWrapper\\": ""
        }
    },
    "extra": {
        "map": [
            [
                "*",
                "M2Coach/MFTFMailHogWrapper"
            ]
        ]
    }
}
